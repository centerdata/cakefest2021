{
    "name": "centerdata/cake-fest",
    "description": "CakeFest plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.2.0",
        "cakephp/migrations": "^3.0",
        "dereuromark/cakephp-ide-helper": "^1.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakeFest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeFest\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
